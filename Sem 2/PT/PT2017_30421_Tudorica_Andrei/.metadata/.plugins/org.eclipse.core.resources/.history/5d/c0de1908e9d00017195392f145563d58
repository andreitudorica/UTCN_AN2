import java.util.*;
public class SinglyListIterator<E> implements ListIterator<E> {
	private int index;
	
	public public SinglyListIterator() {
		this.index=0;
	}
	
	@Override
	public void add(E e) {
		// TODO Auto-generated method stub
		
	}

	@Override
	public boolean hasNext() {
		if(this.next()!=null)
			return true;
		return false;
	}

	@Override
	public boolean hasPrevious() {
		return false;
	}

	@Override
	public E next() {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public int nextIndex() {
		return this.index+1;
	}

	@Override
	public E previous() {
		return null;
	}

	@Override
	public int previousIndex() {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public void remove() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void set(E e) {
		// TODO Auto-generated method stub
		
	}
	
}
